	"homepage": "http://www.potenzaglobalsolutions.com/",
	"type": "wordpress-theme",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Dinesh Kesharwani",
			"email": "dinesh.kesarwani@potenzaglobalsolutions.com",
			"homepage": "https://gitlab.com/dinesh4monto/cardealer"
		}
	],
	"require": {
		"php": ">=5.6"
	},
	"require-dev": {
		"wptrt/wpthemereview": "^0.2.1",
		"phpcompatibility/phpcompatibility-wp": "^2.1.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
		"roave/security-advisories": "dev-master"
	},
	"scripts": {
