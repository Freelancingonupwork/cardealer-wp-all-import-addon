		"format": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf"
	},
	"support": {
		"issues": "https://gitlab.com/dinesh4monto/cardealer/issues",
		"source": "https://gitlab.com/dinesh4monto/cardealer/"
